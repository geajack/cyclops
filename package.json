{
    "name": "computervision",
    "publisher": "Jack Millist",
    "repository": {
        "url": "https://github.com/geajack/vscode-computer-vision-extenstion"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.32.0"
    },
    "main": "extension.js",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "computerVision.closeView",
                "category": "CV",
                "title": "Close"
            },
            {
                "command": "computerVision.switchStackFrame",
                "category": "CV",
                "title": "Use"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "computerVision",
                    "title": "Computer Vision",
                    "icon": "icon.png"
                }
            ]
        },
        "views": {
            "computerVision": [
                {
                    "id": "views",
                    "name": "Views",
                    "visibility": "collapsed",
                    "contextualTitle": "Open views"
                },
                {
                    "id": "stackFrames",
                    "name": "Stack Frames",
                    "visibility": "visible",
                    "contextualTitle": "Active stack frames"
                }
            ]
        },
        "menus": {
            "view/item/context": [
                {
                    "command": "computerVision.closeView",
                    "when": "view == views"
                },
                {
                    "command": "computerVision.switchStackFrame",
                    "group": "inline",
                    "when": "view == stackFrames"
                }
            ],
            "commandPalette": [
                {
                    "command": "computerVision.closeView",
                    "when": "false"
                },
                {
                    "command": "computerVision.switchStackFrame",
                    "when": "false"
                }
            ]
        }
    }
}